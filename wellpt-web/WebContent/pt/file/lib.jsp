<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ include file="/pt/common/taglibs.jsp"%><!DOCTYPE html><html lang="en"><head><title>			<c:if test="${!empty folder.uuid}">				${folder.title}			</c:if>			<c:if test="${empty folder.uuid}">				<spring:message code="fileManager.btn.newLib" />			</c:if></title><%@ include file="/pt/common/meta.jsp"%><%@ include file="/pt/dytable/form_css.jsp"%><link rel="stylesheet" href="${ctx}/resources/pt/css/fileManager.css" /></head><body>	<div class="div_body">		<div class="form_header">			<!--标题-->			<div class="form_title">				<c:if test="${!empty folder.uuid}">				<h2>${folder.title}</h2>				</c:if>				<c:if test="${empty folder.uuid}">					<h2><spring:message code="fileManager.btn.newLib" /></h2>				</c:if>			</div>			<div id="toolbar" class="form_toolbar">				<div class="form_operate">					<c:if test="${haveEdit eq '1'}">						<button type="button" id="libButton"><spring:message code="fileManager.btn.save" /></button>						<c:if test="${!empty folder.uuid}">							<button type="button" id="deleteButton"><spring:message code="fileManager.btn.del" /></button>						</c:if>					</c:if><!-- 					<button type="button" class="btn" onclick="window.close();">关闭</button> -->				</div>			</div>		</div>		<div class="form_content">			<iframe id="lib_iframe" name="lib_iframe"				style="width: 1px; height: 0px;display: none;"></iframe>			<form class="form-horizontal" id="libForm" name="libForm"				action="${ctx}/fileManager/folder/${method}" method="post" target="lib_iframe">				<input name="uuid" id="uuid" type="hidden" value="${folder.uuid}" />				<input name="parentUuid" id="parentUuid" type="hidden"					value="${parentUuid}" />				<input name="rank" value="${folder.rank}" type="hidden" />				<div class="dytable_form">				<div class="post-sign">					<div class="post-detail">				<table width="100%">					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.title" /></td>					    <td class="value">							<input name="title" id="title" size="16" value="${folder.title}"								type="text"/>					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.LibID" /></td>					    <td class="value">					    	<c:if test="${empty folder.uuid}">								<input name="id" id="folderId" size="16" value="${folder.id}"									type="text">							</c:if>							<c:if test="${!empty folder.uuid}">								<input name="id" id="folderId" size="16" value="${folder.id}"									type="hidden">							</c:if>							<c:if test="${!empty folder.uuid}">												${folder.id}											</c:if>					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.seq" /></td>					    <td class="value">							<input id="seqNum" name="seqNum" value="${folder.seqNum}"								size="6" type="text"/>					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.folderManager" /></td>					    <td class="value">							<input type="text" id="libManagerNames" name="libManagerNames"								value="${folder.libManagerNames}" /><input id="libManager"								name="libManager" type="hidden" value="${folder.libManager}" />							<div id="sourcelibManagerValue" style="display: none;">${folder.libManager}</div>							<input type="hidden" id="libManagerChange"								name="libManagerChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.editAllFile" /></td>					    <td class="value">							<input type="text" id="editAllFileNames" name="editAllFileNames"								value="${folder.editAllFileNames}" /><input id="editAllFile"								name="editAllFile" type="hidden" value="${folder.editAllFile}" />							<div id="sourceEditAllFileValue" style="display: none;">${folder.editAllFile}</div>							<input type="hidden" id="editAllFileChange"								name="editAllFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.createAllFile" /></td>					    <td class="value">							<input type="text" id="createAllFileNames"								name="createAllFileNames" value="${folder.createAllFileNames}" /><input								id="createAllFile" name="createAllFile" type="hidden"								value="${folder.createAllFile}" />							<div id="sourceCreateAllFileValue" style="display: none;">${folder.createAllFile}</div>							<input type="hidden" id="createAllFileChange"								name="createAllFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.readAllFile" /></td>					    <td class="value">							<input type="text" id="readAllFileNames" name="readAllFileNames"								value="${folder.readAllFileNames}" /><input id="readAllFile"								name="readAllFile" type="hidden" value="${folder.readAllFile}" />							<div id="sourceReadAllFileValue" style="display: none;">${folder.readAllFile}</div>							<input type="hidden" id="readAllFileChange"								name="readAllFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.superReadAllFile" /></td>					    <td class="value">							<input type="text" id="superReadAllFileNames"								name="superReadAllFileNames" value="${folder.superReadAllFileNames}" /><input								id="superReadAllFile" name="superReadAllFile" type="hidden"								value="${folder.superReadAllFile}" />							<div id="sourceSuperReadAllFileValue" style="display: none;">${folder.superReadAllFile}</div>							<input type="hidden" id="superReadAllFileChange"								name="superReadAllFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.viewSetUp" /></td>					    <td class="value">							<input type="hidden" value="${folder.viewId}" id="viewId" name="viewId"/>							<input type="text" value="${folder.viewName}" size="26" id="viewName" name="viewName"/>					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.fileDynamicForm" /></td>					    <td class="value">							<input type="text" id="templateNames" name="templateNames"								value="${folder.templateNames}" /> <input type="hidden"								id="templateIds" name="templateIds" value="${folder.templateIds}" />							<div id="sourceTemplateValue" style="display: none;">${folder.templateIds}</div>							<input id="templateChange" name="templateChange" type="hidden"								value="0">							<!-- 表单模板是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.flowName" /></td>					    <td class="value">							<input type="text" id="flowName" name="flowName"								value="${folder.flowName}" /> <input type="hidden" id="flowId"								name="flowId" value="${folder.flowId}" />					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.fileCreateMsgFormat" /></td>					    <td class="value">							<input type="text" id="fileCreateMsgFormatName" name="fileCreateMsgFormatName"								value="${folder.fileCreateMsgFormatName}" /> <input type="hidden" id="fileCreateMsgFormatId"								name="fileCreateMsgFormatId" value="${folder.fileCreateMsgFormatId}" />					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.fileEditorMsgFormat" /></td>					    <td class="value">							<input type="text" id="fileEditorMsgFormatName" name="fileEditorMsgFormatName"								value="${folder.fileEditorMsgFormatName}" /> <input type="hidden" id="fileEditorMsgFormatId"								name="fileEditorMsgFormatId" value="${folder.fileEditorMsgFormatId}" />					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.libType" /></td>					    <td class="value">							${libType}					    </td>					</tr>					<tr class="odd">					    <td class="Label"></td>					    <td class="value lib">					    	<span>								<c:choose>									<c:when test="${versionManagerUnEditable eq '1'}">									<c:if test="${folder.versionManagement eq '1'}"><input id="versionManagement" type="checkbox" disabled="disabled" checked="checked" ><input type="hidden" id="versionManagement"										name="versionManagement" value="1"/></c:if>									<c:if test="${folder.versionManagement ne '1'}"><input id="versionManagement" type="checkbox" disabled="disabled" ><input type="hidden" id="versionManagement"										name="versionManagement" value="0"/></c:if>									</c:when>									<c:otherwise>									<input type="checkbox" id="versionManagement"										name="versionManagement" value="1"										<c:if test="${folder.versionManagement eq '1'}">checked="checked"</c:if>										id="versionManagement" />									</c:otherwise>								</c:choose>								<label for="versionManagement"><spring:message code="fileManager.label.versionManagement" /></label>							</span>							<span>								<input type="checkbox" name="saveNoticeReader" value="1"									<c:if test="${folder.saveNoticeReader eq '1'}">checked="checked"</c:if>									id="saveNoticeReader" />								<label for="saveNoticeReader"><spring:message code="fileManager.label.saveNotice" /></label>							</span>							<span>								<input type="checkbox" name="canPrint" value="1"									<c:if test="${folder.canPrint eq '1'}">checked="checked"</c:if>									id="canPrint" />								<label for="canPrint"><spring:message code="fileManager.label.enablePrint" /></label>							</span>							<span>								<input type="checkbox" name="canDownload" value="1"									<c:if test="${folder.canDownload eq '1'}">checked="checked"</c:if>									id="canDownload" />								<label for="canDownload"><spring:message code="fileManager.label.enableDownload" /></label>							</span>					    </td>					</tr>					<tr>					    <td class="Label"></td>					    <td class="value">					    </td>					</tr>				</table>				</div>				</div>				</div>			</form>		</div>		<div>			<div id="treeDialog">				<ul id="folder_tree" class="ztree"></ul>			</div>		</div>	</div>	<div class="body_foot"></div>			<!-- /container -->	<script type="text/javascript" src="${ctx}/resources/jquery/jquery.js"></script>	<script type="text/javascript"		src="${ctx}/resources/jqueryui/js/jquery-ui.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/org/unit/jquery.unit.js'></script>	<script type="text/javascript"		src="${ctx}/resources/bootstrap/js/bootstrap.js"></script>	<script type="text/javascript" src="${ctx}/resources/utils/json2.js"></script>	<script type="text/javascript" src="${ctx}/resources/pt/js/global.js"></script>	<script type="text/javascript"		src="${ctx}/resources/ztree/js/jquery.ztree.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.comboTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.dytableTree.js'></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.folderTree.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.workflowComboTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.messageTemplateTree.js'></script> 	<script type="text/javascript" src="${ctx}/resources/pt/js/common/jquery.dyviewTree.js">		</script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.alerts.js"></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.cmsWindown.js"></script>	<script type="text/javascript" src="${ctx}/resources/timers/jquery.timers.min.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/file/folder.js'></script>	<script type="text/javascript">	$(function(){		$("#lib_iframe").load(function(e) {			oAlert(fileManager.saveSuccess, function(){				var parentUuid =  $("#parentUuid").val();				 refleshParentWinByFolder(parentUuid);			});		});	});	</script></body></html>