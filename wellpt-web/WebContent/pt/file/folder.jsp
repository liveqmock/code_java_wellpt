<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><%@ include file="/pt/common/taglibs.jsp"%><!DOCTYPE html><html lang="en"><head><title><c:if test="${!empty folder.uuid}">				${folder.title}			</c:if>			<c:if test="${empty folder.uuid}">				<spring:message code="fileManager.btn.newFolder" />			</c:if></title><%@ include file="/pt/common/meta.jsp"%><%@ include file="/pt/dytable/form_css.jsp"%><link rel="stylesheet" href="${ctx}/resources/pt/css/fileManager.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/ztree/css/zTreeStyle/zTreeStyle.css" /></head><body>		<div class="div_body">		<div class="form_header">			<!--标题-->			<div class="form_title">			<c:if test="${!empty folder.uuid}">				<h2>${folder.title}</h2>			</c:if>			<c:if test="${empty folder.uuid}">				<h2><spring:message code="fileManager.btn.newFolder" /></h2>			</c:if>			</div>			<div id="toolbar" class="form_toolbar">				<div class="form_operate">					<c:if test="${haveEdit eq '1'}">						<!-- 创建权限，包括编辑权限！@ -->						<button type="button" id="folderButton"><spring:message code="fileManager.btn.save" /></button>						<c:if test="${!empty folder.uuid}">							<button type="button" id="deleteButton"><spring:message code="fileManager.btn.del" /></button>						</c:if>					</c:if><!-- 					<button type="button" class="btn" onclick="window.close();">关闭</button> -->				</div>			</div>		</div>		<div class="form_content">			<iframe id="folder_iframe" name="folder_iframe"					style="width: 1px; height: 0px;display: none;"></iframe>			<form class="form-horizontal" id="folderForm" name="folderForm"				action="${ctx}/fileManager/folder/${method}" method="post" target="folder_iframe">				<input name="parentUuid" id="parentUuid" type="hidden"					value="${parentUuid}" />				<input name="uuid" id="uuid" type="hidden" value="${folder.uuid}" />				<input name="versionManagement" type="hidden" value="${folder.versionManagement}">				<input name="saveNoticeReader" type="hidden" value="${folder.saveNoticeReader}">				<input name="canPrint" type="hidden" value="${folder.canPrint}">				<input name="canDownload" type="hidden" value="${folder.canDownload}">				<div class="dytable_form">				<div class="post-sign">					<div class="post-detail">				<table width="100%">					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.title" /></td>					    <td class="value">							<input id="title" name="title" value="${folder.title}" size="16"									type="text"/>					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.seq" /></td>					    <td class="value">							<input id="seqNum" name="seqNum" value="${folder.seqNum}"								size="6" type="text"/>					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.remark" /></td>					    <td class="value">							<textarea id="remark" name="remark" rows="3">${folder.remark}</textarea>					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.folderManager" /></td>					    <td class="value">							<input type="text" id="folderManagerNames"								name="folderManagerNames" value="${folder.folderManagerNames}" /><input								id="folderManager" name="folderManager" type="hidden"								value="${folder.folderManager}" />							<div id="sourceFolderManagerValue" style="display: none;">${folder.folderManager}</div>							<input type="hidden" id="folderManagerChange"								name="folderManagerChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.editFolderFile" /></td>					    <td class="value">							<input type="text" id="editFolderFileNames"								name="editFolderFileNames" value="${folder.editFolderFileNames}" /><input								id="editFolderFile" name="editFolderFile" type="hidden"								value="${folder.editFolderFile}" />							<div id="sourceEditFolderFileValue" style="display: none;">${folder.editFolderFile}</div>							<input type="hidden" id="editFolderFileChange"								name="editFolderFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.createFolderFile" /></td>					    <td class="value">							<input type="text" id="createFolderFileNames"								name="createFolderFileNames" value="${folder.createFolderFileNames}" /><input								id="createFolderFile" name="createFolderFile" type="hidden"								value="${folder.createFolderFile}" />							<div id="sourceCreateFolderFileValue" style="display: none;">${folder.createFolderFile}</div>							<input type="hidden" id="createFolderFileChange"								name="createFolderFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.readFolderFile" /></td>					    <td class="value">							<input type="text" id="readFolderFileNames"								name="readFolderFileNames" value="${folder.readFolderFileNames}" /><input								id="readFolderFile" name="readFolderFile" type="hidden"								value="${folder.readFolderFile}" />							<div id="sourceReadFolderFileValue" style="display: none;">${folder.readFolderFile}</div>							<input type="hidden" id="readFolderFileChange"								name="readFolderFileChange" value="0">							<!-- 人员是否发生变更 -->					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.viewSetUp" /></td>					    <td class="value">							<input type="hidden" value="${folder.viewId}" id="viewId" name="viewId"/>							<input type="text" value="${folder.viewName}" size="26" id="viewName" name="viewName"/>					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.fileDynamicForm" /></td>					    <td class="value">							<input type="text" id="templateNames" name="templateNames"								value="${folder.templateNames}" /><input id="templateIds"								name="templateIds" type="hidden" value="${folder.templateIds}" />							<div id="sourceTemplateValue" style="display: none;">${folder.templateIds}</div>							<input type="hidden" id="templateChange" name="templateChange"								value="0">							<!-- 表单模板是否发生变更 -->					    </td>					</tr>					<tr>					    <td class="Label"><spring:message code="fileManager.label.fileCreateMsgFormat" /></td>					    <td class="value">							<input type="text" id="fileCreateMsgFormatName" name="fileCreateMsgFormatName"								value="${folder.fileCreateMsgFormatName}" /> <input type="hidden" id="fileCreateMsgFormatId"								name="fileCreateMsgFormatId" value="${folder.fileCreateMsgFormatId}" />					    </td>					</tr>					<tr class="odd">					    <td class="Label"><spring:message code="fileManager.label.fileEditorMsgFormat" /></td>					    <td class="value">							<input type="text" id="fileEditorMsgFormatName" name="fileEditorMsgFormatName"								value="${folder.fileEditorMsgFormatName}" /> <input type="hidden" id="fileEditorMsgFormatId"								name="fileEditorMsgFormatId" value="${folder.fileEditorMsgFormatId}" />					    </td>					</tr>										<tr>					    <td class="Label"></td>					    <td class="value">				    </td>				</tr>									</table>				</div>				</div>				</div>			</form>		</div>		<div id="treeDialog">				<ul id="folder_tree" class="ztree"></ul>			</div>	</div>	<div class="body_foot"></div>	<!-- /container -->	<script type="text/javascript" src="${ctx}/resources/jquery/jquery.js"></script>	<script type="text/javascript"		src="${ctx}/resources/jqueryui/js/jquery-ui.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/org/unit/jquery.unit.js'></script>	<script type="text/javascript"		src="${ctx}/resources/bootstrap/js/bootstrap.js"></script>	<script type="text/javascript" src="${ctx}/resources/utils/json2.js"></script>	<script type="text/javascript" src="${ctx}/resources/pt/js/global.js"></script>	<script type="text/javascript"		src="${ctx}/resources/ztree/js/jquery.ztree.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.comboTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.dytableTree.js'></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.dyviewTree.js"></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.folderTree.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.messageTemplateTree.js'></script>			<script type="text/javascript"		src='${ctx}/resources/pt/js/file/folder.js'></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.alerts.js"></script>	<script type="text/javascript"		src="${ctx}/resources/pt/js/common/jquery.cmsWindown.js"></script>	<script type="text/javascript" src="${ctx}/resources/timers/jquery.timers.min.js"></script>	<script type="text/javascript">	$(function(){		$("#folder_iframe").load(function(e) {			oAlert(fileManager.saveSuccess, function(){				var parentUuid =  $("#parentUuid").val();				 refleshParentWinByFolder(parentUuid);			});		});	});	</script></body></html>