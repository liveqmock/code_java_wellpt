<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><!DOCTYPE html><%@ include file="/pt/common/taglibs.jsp"%><html lang="en"><head><title><spring:message code="fileManager.label.fileManager" /></title><%@ include file="/pt/common/meta.jsp"%><!-- Bootstrap --><link rel="stylesheet" href="${ctx}/resources/pt/css/fileManager.css" /><link rel="stylesheet"	href="${ctx}/resources/bootstrap/css/bootstrap.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/jqueryui/css/base/jquery-ui.css" /><link rel="stylesheet" type="text/css" 	href="${ctx}/resources/ztree/css/zTreeStyle/zTreeStyle.css" /><link rel="stylesheet" type="text/css" media="screen"	href="${ctx}/resources/pt/css/form.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/pt/css/custom-admin-style.css" /></head><body>	<div class="container-fluid">		<div class="form_header">			<!--标题-->			<div class="form_title">				<h2>${folder.title}</h2>			</div>			<div id="toolbar" class="form_toolbar">				<div class="form_operate">									<button type="button" class="btn" onclick="window.close();"><spring:message code="fileManager.btn.close" /></button>					 				</div>			</div>		</div>		<div class="form_content">			<iframe id="lib_iframe" name="lib_iframe"				style="width: 1px; height: 0px;"></iframe>			<form class="form-horizontal" id="libForm" name="libForm"				action="${ctx}/folder/${method}" method="post" target="lib_iframe">				<input name="uuid" id="uuid" type="hidden" value="${folder.uuid}" />				<input name="parentUuid" id="parentUuid" type="hidden"					value="${parentUuid}" />				<fieldset>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.title" /></label>						<div class="controls">							${folder.title}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.LibID" /></label>						<div class="controls">							${folder.id}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.seq" /></label>						<div class="controls">							${folder.seqNum}						</div>					</div>										<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.folderManager" /></label>						<div class="controls">							${libManager[1]}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.editAllFile" /></label>						<div class="controls">							${editAllFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.createAllFile" /></label>						<div class="controls">							${createAllFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.readAllFile" /></label>						<div class="controls">							${readAllFile[0]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.superReadAllFile" /></label>						<div class="controls">							${superReadAllFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>										<c:if test="${!empty folder.uuid}">					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.viewSetUp" /></label>						<div class="controls">							${folder.viewName}						</div>					</div>					</c:if>										<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.fileDynamicForm" /></label>						<div class="controls">							${templates[1]}							<!-- 表单模板是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.flowName" /></label>						<div class="controls">							${folder.flowName}						</div>					</div>										<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.libType" /></label>						<div class="controls">${libType}</div>					</div>										<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.versionManagement" /></label>						<div class="controls">							<c:if test="${folder.versionManagement eq '1'}"><spring:message code="fileManager.label.versionManagementEnable" /></c:if>							<c:if test="${folder.versionManagement ne '1'}"><spring:message code="fileManager.label.versionManagementDisable" /></c:if>						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.saveNotice" /></label>						<div class="controls">							<c:if test="${folder.saveNoticeReader eq '1'}"><spring:message code="fileManager.label.yes" /></c:if>							<c:if test="${folder.saveNoticeReader ne '1'}"><spring:message code="fileManager.label.no" /></c:if>						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.enablePrint" /></label>						<div class="controls">							<c:if test="${folder.canPrint eq '1'}"><spring:message code="fileManager.label.enable" /></c:if>							<c:if test="${folder.canPrint ne '1'}"><spring:message code="fileManager.label.disable" /></c:if>						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.enableDownload" /></label>						<div class="controls">							<c:if test="${folder.canDownload eq '1'}"><spring:message code="fileManager.label.enable" /></c:if>							<c:if test="${folder.canDownload ne '1'}"><spring:message code="fileManager.label.disable" /></c:if>						</div>					</div>									</fieldset>			</form>		</div>	</div>	<!-- /container -->	<script type="text/javascript" src="${ctx}/resources/jquery/jquery.js"></script>	<script type="text/javascript"		src="${ctx}/resources/jqueryui/js/jquery-ui.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/org/unit/jquery.unit.js'></script>	<script type="text/javascript"		src="${ctx}/resources/bootstrap/js/bootstrap.js"></script>	<script type="text/javascript" src="${ctx}/resources/utils/json2.js"></script>	<script type="text/javascript" src="${ctx}/resources/pt/js/global.js"></script>	<script type="text/javascript"		src="${ctx}/resources/ztree/js/jquery.ztree.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.comboTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.dytableTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/common/jquery.workflowComboTree.js'></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/file/folder.js'></script>	<script type="text/javascript">	$(function(){		$("#lib_iframe").load(function(e) {			alert(fileManager.saveSuccess);			window.close();		});	});	</script></body></html>