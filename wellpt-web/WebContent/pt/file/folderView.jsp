<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><!DOCTYPE html><%@ include file="/pt/common/taglibs.jsp"%><html lang="en"><head><title><spring:message code="fileManager.label.fileManager" /></title><%@ include file="/pt/common/meta.jsp"%><link rel="stylesheet" href="${ctx}/resources/pt/css/fileManager.css" /><link rel="stylesheet"	href="${ctx}/resources/bootstrap/css/bootstrap.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/jqueryui/css/base/jquery-ui.css" /><link rel="stylesheet" type="text/css" media="screen"	href="${ctx}/resources/pt/css/form.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/pt/css/custom-admin-style.css" /></head><body>	<div class="container-fluid">		<div class="form_header">			<!--标题-->			<div class="form_title">				<h2>${folder.title}</h2>			</div>			<div id="toolbar" class="form_toolbar">				<div class="form_operate">					<button type="button" class="btn" onclick="window.close();"><spring:message code="fileManager.btn.close" /></button>				</div>			</div>		</div>		<div class="form_content">			<iframe id="folder_iframe" name="folder_iframe"					style="width: 1px; height: 0px;"></iframe>			<form class="form-horizontal" id="folderForm" name="folderForm"				action="${ctx}/folder/${method}" method="post" target="folder_iframe">				<input name="uuid" id="uuid" type="hidden" value="${folder.uuid}" />				<input name="parentUuid" id="parentUuid" type="hidden"					value="${parentUuid}" />				<fieldset>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.title" /></label>						<div class="controls">							${folder.title}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.seq" /></label>						<div class="controls">							${folder.seqNum}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.remark" /></label>						<div class="controls">							${folder.remark}						</div>					</div>										<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.folderManager" /></label>						<div class="controls">							${folderManager[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.editFolderFile" /></label>						<div class="controls">							${editFolderFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.createFolderFile" /></label>						<div class="controls">							${createFolderFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.readFolderFile" /></label>						<div class="controls">							${readFolderFile[1]}							<!-- 人员是否发生变更 -->						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.viewSetUp" /></label>						<div class="controls">							${folder.viewName}						</div>					</div>					<div class="control-group">						<label class="control-label"><spring:message code="fileManager.label.fileDynamicForm" /></label>						<div class="controls">							${templates[1]}							<!-- 表单模板是否发生变更 -->						</div>					</div>				</fieldset>			</form>		</div>	</div>	<!-- /container -->	<script type="text/javascript" src="${ctx}/resources/jquery/jquery.js"></script>	<script type="text/javascript"		src="${ctx}/resources/jqueryui/js/jquery-ui.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/org/unit/jquery.unit.js'></script>	<script type="text/javascript"		src="${ctx}/resources/bootstrap/js/bootstrap.js"></script>	<script type="text/javascript" src="${ctx}/resources/utils/json2.js"></script>	<script type="text/javascript" src="${ctx}/resources/pt/js/global.js"></script>	<script type="text/javascript"		src='${ctx}/resources/pt/js/file/folder.js'></script>	</body></html>