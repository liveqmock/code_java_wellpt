<%@ page language="java" contentType="text/html; charset=UTF-8"	pageEncoding="UTF-8"%><!DOCTYPE html><html lang="en"><head><title>文件管理</title><%@ include file="/pt/common/taglibs.jsp"%><style type="text/css"></style><%@ include file="/pt/common/meta.jsp"%><link rel="stylesheet"	href="${ctx}/resources/pt/css/fileManager.css" /><link rel="stylesheet"	href="${ctx}/resources/bootstrap/css/bootstrap.min.css" /><link rel="stylesheet"	href="${ctx}/resources/bootstrap/css/bootstrap-responsive.min.css" /><link rel="stylesheet" type="text/css"	href="${ctx}/resources/jqueryui/css/base/jquery-ui.css" /><script type="text/javascript" src="${ctx}/resources/jquery/jquery.js"></script><script type="text/javascript" src="${ctx}/resources/jqueryui/js/jquery-ui.js"></script><script type="text/javascript" src='${ctx}/resources/pt/js/org/unit/jquery.unit.js'></script><script type="text/javascript" src='${ctx}/resources/pt/js/file/folder.js'></script></head><body>	<div class="container-fluid">		 <div>				<ul class="breadcrumb">					<li>						<a href="${ctx}/folder/indexList?id=">文件管理</a> <span class="divider">/</span>					</li>					<li>						<a href="${ctx}/folder/indexList?id=${folder.uuid}">文件夹</a>					</li>				</ul>			</div>		 <form class="form-horizontal" id="folderForm" name="folderForm"  action="${ctx}/folder/${method}" method="post">		  <input name="uuid" id="uuid" type="hidden" value="${folder.uuid}"/>		  <input name="parentUuid" id="parentUuid" type="hidden" value="${parentUuid}" />								<fieldset>									<div class="control-group">										<label class="control-label" >标题</label>										<div class="controls">											<input id="title"												name="title" value="${folder.title}"	size="16" type="text">										</div>									</div>								   <div class="control-group">										<label class="control-label" >序号</label>										<div class="controls">											<input id="seqNum"												name="seqNum" value="${folder.seqNum}"	size="6" type="text">										</div>									</div>									<div class="control-group">										<label class="control-label" >备注</label>										<div class="controls">											<textarea id="remark" value="${folder.remark}" name="remark" rows="3"></textarea>										</div>									</div>																											<div class="control-group">										<label class="control-label" >夹管理者</label>										<div class="controls">											<input type="text" id="folderManagerNames" name="folderManagerNames"    value="${folderManager[1]}" /><input   id="folderManager" name="folderManager" type="hidden" value="${folderManager[0]}" />											<div id="sourceFolderManagerValue" style="display: none;">${folderManager[0]}</div>											<input type="hidden" id="folderManagerChange" name="folderManagerChange" value="0"><!-- 人员是否发生变更 -->										</div>									</div>																		<div class="control-group">										<label class="control-label" >可以编辑夹文档的人</label>										<div class="controls">											<input type="text" id="editFolderFileNames" name="editFolderFileNames"    value="${editFolderFile[1]}" /><input   id="editFolderFile" name="editFolderFile" type="hidden" value="${editFolderFile[0]}" />											<div id="sourceEditFolderFileValue" style="display: none;">${editFolderFile[0]}</div>											<input type="hidden" id="editFolderFileChange" name="editFolderFileChange" value="0"><!-- 人员是否发生变更 -->										</div>									</div>																		<div class="control-group">										<label class="control-label" >可以创建夹文档的人</label>										<div class="controls">											<input type="text" id="createFolderFileNames" name="createFolderFileNames"    value="${createFolderFile[1]}" /><input   id="createFolderFile" name="createFolderFile" type="hidden" value="${createFolderFile[0]}" />											<div id="sourceCreateFolderFileValue" style="display: none;">${createFolderFile[0]}</div>											<input type="hidden" id="createFolderFileChange" name="createFolderFileChange" value="0"><!-- 人员是否发生变更 -->										</div>									</div>																																				<div class="control-group">										<label class="control-label" >可以阅读夹文档的人</label>										<div class="controls">											<input type="text" id="readFolderFileNames" name="readFolderFileNames"    value="${readFolderFile[1]}" /><input   id="readFolderFile" name="readFolderFile" type="hidden" value="${readFolderFile[0]}" />											<div id="sourceReadFolderFileValue" style="display: none;">${readFolderFile[0]}</div>											<input type="hidden" id="readFolderFileChange" name="readFolderFileChange" value="0"><!-- 人员是否发生变更 -->										</div>									</div>																											<div class="control-group">										<label class="control-label" >文件动态表单</label>										<div class="controls">											<input type="text" id="templateNames" name="templateNames"    value="${templates[1]}" /><input   id="templateIds" name="templateIds" type="hidden" value="${templates[0]}" />											<input type="hidden" id="templateRemarks" name="templateRemarks"    value="${templates[2]}" />											<div id="sourceTemplateValue" style="display: none;">${templates[0]}</div>											<input type="hidden" id="templateChange" name="templateChange" value="0"><!-- 表单模板是否发生变更 -->										</div>									</div>																											<div class="form-actions">										<c:if test="${haveEdit eq '1'}"><!-- 创建权限，包括编辑权限！@ -->										<button type="button" id="folderButton" class="btn btn-primary">保存</button>										<c:if test="${!empty folder.uuid}">										<button type="button" id="deleteButton" class="btn btn-primary">删除</button>										</c:if>										</c:if>										<button type="button" class="btn" onclick="window.location.href='${ctx}/folder/indexList?id=${parentUuid}'" >返回</button>									</div>								</fieldset>							</form>				 	</div>	<!-- /container -->	<script src="${ctx}/resources/bootstrap/js/bootstrap.min.js"></script>	<script type="text/javascript" src="${ctx}/resources/utils/json2.js"></script>	<script type="text/javascript" src="${ctx}/resources/pt/js/global.js"></script></body></html>